{
    "name": "passionweb/custom-form-finisher",
    "type": "typo3-cms-extension",
    "description": "Shows the usage and implementation of custom form finishers by an example finisher that strip tags of entered message field.\n",
    "version": "1.0.0",
    "homepage": "https://www.passionweb.de",
    "authors": [
        {
            "name": "Manuel Schnabel",
            "role": "Developer"
        }
    ],
    "license": "GPL-2.0-or-later",
    "require": {
        "typo3/cms-core": "^12.4",
        "typo3/cms-form": "^12.4"
    },
    "autoload": {
        "psr-4": {
            "Passionweb\\CustomFormFinisher\\": "Classes"
        }
    },
    "replace": {
        "typo3-ter/custom-form-finisher": "self.version"
    },
    "extra": {
        "typo3/cms": {
            "extension-key": "custom_form_finisher"
        }
    }
}
